{
  "name": "babilonia-express-vuejs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "run-p vue-serve server",
    "vue-serve": "vue-cli-service serve --port 8000",
    "server": "node --loader ts-node/esm --experimental-specifier-resolution=node server/index.ts",
    "dev:start": "run-p dev:vue-serve dev:server",
    "dev:vue-serve": "vue-serve",
    "dev:server": "nodemon server/index.ts",
    "test:start": "run-p test:vue-serve test:server",
    "test:vue-serve": "vue-cli-service serve --mode test --port 8888",
    "test:server": "node --loader ts-node/esm --experimental-specifier-resolution=node server/index.ts --test-mode",
    "test:dev:start": "run-p test:dev:vue-serve test:server",
    "test:dev:vue-serve": "test:vue-serve",
    "test:dev:server": "nodemon server/index.ts --test-mode",
    "test:api": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:app": "cypress run"
  },
  "dependencies": {
    "@cypress/webpack-preprocessor": "^5.12.2",
    "@types/express": "^4.17.14",
    "@types/sqlite3": "^3.1.8",
    "ajv": "^8.11.0",
    "bootstrap": "^5.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "node-fetch": "^3.2.10",
    "run-p": "^0.0.0",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.1",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.4",
    "@types/cors": "^2.8.12",
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-e2e-cypress": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.2.39",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.0.2",
    "babel-preset-minify": "^0.5.2",
    "cypress": "^10.8.0",
    "cypress-fail-on-console-error": "^3.3.1",
    "eslint": "^8.24.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-xo": "^0.42.0",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.5.1",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.3.2",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "typescript-json-validator": "^2.4.2"
  }
}
