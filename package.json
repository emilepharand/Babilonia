{
  "name": "babilonia-express-vuejs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "run-p vue-serve server",
    "vue-serve": "vue-cli-service serve --port 8000",
    "server": "node --loader ts-node/esm --experimental-specifier-resolution=node server/index.ts --test-mode",
    "dev:start": "run-p dev:vue-serve dev:server",
    "dev:vue-serve": "vue-serve",
    "dev:server": "nodemon server/index.ts",
    "test:start": "run-p test:vue-serve test:server",
    "test:vue-serve": "vue-cli-service serve --mode test --port 8888",
    "test:server": "node --loader ts-node/esm --experimental-specifier-resolution=node server/index.ts --test-mode",
    "test:dev:start": "run-p test:dev:vue-serve test:server",
    "test:dev:vue-serve": "test:vue-serve",
    "test:dev:server": "nodemon server/index.ts --test-mode",
    "test:api": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:app": "cypress run"
  },
  "dependencies": {
    "@cypress/webpack-preprocessor": "^5.11.1",
    "@types/express": "^4.17.13",
    "@types/sqlite3": "^3.1.8",
    "ajv": "^8.10.0",
    "bootstrap": "^5.1.3",
    "core-js": "^3.21.1",
    "cors": "^2.8.5",
    "cypress-fail-on-console-error": "^2.1.3",
    "express": "^4.17.3",
    "node-fetch": "^3.2.3",
    "run-p": "^0.0.0",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.12",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.4.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vue/cli-plugin-babel": "~5.0.1",
    "@vue/cli-plugin-e2e-cypress": "~5.0.1",
    "@vue/cli-plugin-eslint": "~5.0.1",
    "@vue/cli-plugin-router": "~5.0.1",
    "@vue/cli-plugin-typescript": "~5.0.1",
    "@vue/cli-service": "~5.0.1",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "babel-preset-minify": "^0.5.1",
    "cypress": "^10.8.0",
    "eslint": "^8.11.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-xo": "^0.40.0",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^8.5.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "superagent": "^7.1.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "typescript-json-validator": "^2.4.2"
  }
}
