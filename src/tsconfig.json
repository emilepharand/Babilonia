{
    "compilerOptions": {
      "module": "esnext",
      "target": "es2021",
      "strict": true,
      "jsx": "preserve",
      "importHelpers": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "node",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "inlineSourceMap": true,
      "baseUrl": ".",
      "types": [
        "webpack-env",
        "cypress",
        "vite/client"
      ],
      "lib": [
        "esnext",
        "dom",
        "dom.iterable",
        "scripthost"
      ]
    },
    "include": [
      "**/*.vue"
    ]
  }
  